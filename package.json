{
	"name": "remotv",
	"version": "0.0.1",
	"private": true,
	"main": "electron/main.cjs",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"build:overlay": "vite build --config vite.config.overlay.js",
		"dev:overlay": "vite build --watch --config vite.config.overlay.js ",
		"dev:gui:all": "npm run dev:overlay && npm run dev",
		"dist": "electron-builder",
		"deve": "electron ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/express": "^4.17.14",
		"electron": "^21.2.2",
		"electron-builder": "^23.6.0",
		"sass": "^1.55.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0-beta.1"
	},
	"dependencies": {
		"@cliqz/adblocker-electron": "^1.25.1",
		"@sveltejs/adapter-static": "^1.0.0-next.41",
		"ace-builds": "^1.12.4",
		"axios": "^1.1.3",
		"browser-image-compression": "^2.0.0",
		"const": "^1.0.0",
		"cors": "^2.8.5",
		"cross-fetch": "^3.1.5",
		"express": "^4.18.2",
		"loudness": "^0.4.1",
		"svelte-ace": "^1.0.21",
		"svelte-icons": "^2.1.0"
	},
	"author": {
		"email": "felbsn@gmail.com",
		"name": "felbsn",
		"url": "https://github.com/felbsn"
	},
	"license": "CC0-1.0",
	"build": {
		"files": [
			"build/**/*",
			"templates/**/*",
			"electron/**/*"
		],
		"extraResources": [],
		"extraFiles": [],
		"appId": "com.decagram.remotv",
		"mac": {
			"category": "your.app.category.type"
		},
		"nsis": {
			"oneClick": false
		}
	}
}
